    <rdf:Description rdf:about="http://purl.org/DP/preservation-case#hasComparisonMetric">
      <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://purl.org/DP/preservation-case#hasComparisonMetric">
